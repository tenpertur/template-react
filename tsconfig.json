{
  "compilerOptions":{
    "outDir":"./dist",
    "noImplicitAny":true,
    "module":"es6",
    "target":"es5",
    "jsx":"react-jsx",
    "allowJs": true,
    "moduleResolution":"node",
    "esModuleInterop": true,
    "baseUrl": "src",
    "lib": [
      "dom",
      "es2015"
    ],
    "paths": {
      "components/*": [
        "components/*"
      ]
    }
  },
  "include": ["src"]
}
